{
	"name": "notes",
	"version": "0.0.1",
	"scripts": {
		"obsidian": "node scripts/prepare-obsidian.js",
		"dev": "pnpm obsidian && svelte-kit dev",
		"build": "pnpm obsidian && svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && husky install",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier  --write --plugin-search-dir=. .",
		"validate": "svelte-check --fail-on-hints --ignore 'src/lib/components/Graph.svelte'"
	},
	"devDependencies": {
		"@playwright/test": "1.22.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "1.0.0-next.324",
		"@tailwindcss/typography": "0.5.2",
		"@types/lodash": "4.14.182",
		"@typescript-eslint/eslint-plugin": "5.27.1",
		"@typescript-eslint/parser": "5.27.1",
		"autoprefixer": "10.4.7",
		"d3-drag": "3.0.0",
		"d3-force": "3.0.0",
		"d3-scale": "4.0.2",
		"d3-scale-chromatic": "3.0.0",
		"d3-selection": "3.0.0",
		"d3-zoom": "3.0.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "3.4.1",
		"husky": "7.0.4",
		"lint-staged": "12.5.0",
		"lodash": "4.17.21",
		"mdsvex": "0.10.6",
		"postcss": "8.4.14",
		"postcss-load-config": "3.1.4",
		"prettier": "2.6.2",
		"prettier-plugin-svelte": "2.7.0",
		"prettier-plugin-tailwindcss": "0.1.11",
		"remark-wiki-link": "0.0.4",
		"svelte": "3.48.0",
		"svelte-check": "2.7.2",
		"svelte-preprocess": "4.10.7",
		"tailwindcss": "3.1.2",
		"tslib": "2.4.0",
		"typescript": "4.6.4",
		"unist-util-visit": "4.1.0"
	},
	"type": "module",
	"lint-staged": {
		"*.{cjs,js,ts,css,scss,svelte}": [
			"eslint --fix",
			"prettier --write --plugin-search-dir=. ."
		]
	},
	"dependencies": {
		"gray-matter": "4.0.3"
	}
}
